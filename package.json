    "next": "15.4.1",
    "openai": "^5.9.0",
    "puppeteer": "^24.12.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "ts-node": "^10.9.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
